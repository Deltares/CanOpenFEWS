<gridDisplay xmlns="http://www.wldelft.nl/fews" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.wldelft.nl/fews
http://fews.wldelft.nl/schemas/version1.0/gridDisplay.xsd">
	<title>Spatial Display</title>
	<defaults>
		<plotGroupId>WSC_Scalar_Download</plotGroupId>
		<geoMap>
			<geoDatum>WGS 1984</geoDatum>
			<projection>web_mercator</projection>
			<defaultExtent id="Alberta" name="Alberta">
				<left>-128</left>
				<right>-107</right>
				<top>58</top>
				<bottom>49.5</bottom>
			</defaultExtent>
			<extraExtent id="Bow Watershed">
				<left>-117</left>
				<right>-111</right>
				<top>53</top>
				<bottom>50</bottom>
			</extraExtent>
			<extraExtent id="Canada">
				<left>-152</left>
				<right>-47</right>
				<top>40</top>
				<bottom>75</bottom>
			</extraExtent>
			<scaleBarVisible>true</scaleBarVisible>
			<northArrowVisible>true</northArrowVisible>
			<labelsVisible>true</labelsVisible>
			<backgroundColor>white</backgroundColor>
			<openStreetMapLayer id="Open Street Map">
				<url>http://tile.openstreetmap.org</url>
				<visible>true</visible>
				<cacheDir>$MAPLAYERSCACHE_FOLDER$/OSM</cacheDir>
			</openStreetMapLayer>
			<layer id="Satellite">
				<className>nl.wldelft.libx.openmap.GenericTileServerLayer</className>
				<visible>false</visible>
				<properties>
					<string key="tileUrlPattern" value="http://h%RIGHT(QUAD_KEY,1)%.ortho.tiles.virtualearth.net/tiles/h%QUAD_KEY%.jpeg?g=1"/>
					<string key="cacheDir" value="$MAPLAYERSCACHE_FOLDER$/Satellite_Cache"/>
					<int key="minZoomLevel" value="2"/>
					<int key="maxZoomLevel" value="17"/>
					<int key="topZoomLevel" value="19"/>
					<int key="tileSize" value="256"/>
				</properties>
			</layer>
			<layer id="Topografie" name="ArcGIS World Topo Map">
				<className>nl.wldelft.libx.openmap.GenericTileServerLayer</className>
				<visible>true</visible>
				<properties>
					<string key="tileUrlPattern" value="http://server.arcgisonline.com/ArcGIS/rest/services/World_Topo_Map/MapServer/tile/%ZOOM%/%ROW%/%COLUMN%"/>
					<string key="cacheDir" value="$MAPLAYERSCACHE_FOLDER$/Esri_topo"/>
					<int key="minZoomLevel" value="1"/>
					<int key="maxZoomLevel" value="19"/>
					<int key="topZoomLevel" value="21"/>
					<int key="tileSize" value="256"/>
				</properties>
			</layer>
			<layer id="Canvas" name="ArcGIS Canvas World LightGray Base">
				<className>nl.wldelft.libx.openmap.GenericTileServerLayer</className>
				<visible>false</visible>
				<properties>
					<string key="tileUrlPattern" value="http://server.arcgisonline.com/arcgis/rest/services/Canvas/World_Light_Gray_Base/MapServer/tile/%ZOOM%/%ROW%/%COLUMN%"/>
					<string key="cacheDir" value="$MAPLAYERSCACHE_FOLDER$/Esri_Canvas_World_Light_Gray_Base"/>
					<int key="minZoomLevel" value="1"/>
					<int key="maxZoomLevel" value="19"/>
					<int key="topZoomLevel" value="21"/>
					<int key="tileSize" value="256"/>
				</properties>
			</layer>
			<layer id="World" name="ArcGIS World Satellite Map">
				<className>nl.wldelft.libx.openmap.GenericTileServerLayer</className>
				<visible>true</visible>
				<properties>
					<string key="tileUrlPattern" value="http://server.arcgisonline.com/arcgis/rest/services/World_Imagery/MapServer/tile/%ZOOM%/%ROW%/%COLUMN%"/>
					<string key="cacheDir" value="$MAPLAYERSCACHE_FOLDER$/World_Imagery"/>
					<int key="minZoomLevel" value="1"/>
					<int key="maxZoomLevel" value="19"/>
					<int key="topZoomLevel" value="21"/>
					<int key="tileSize" value="256"/>
				</properties>
			</layer>
			<layer id="NatGeo_World_Map" name="ArcGIS Physical Map">
				<className>nl.wldelft.libx.openmap.GenericTileServerLayer</className>
				<visible>false</visible>
				<properties>
					<string key="tileUrlPattern" value="http://server.arcgisonline.com/ArcGIS/rest/services/NatGeo_World_Map/MapServer/tile/%ZOOM%/%ROW%/%COLUMN%"/>
					<string key="cacheDir" value="$MAPLAYERSCACHE_FOLDER$/NatGeo_World_Map"/>
					<int key="minZoomLevel" value="1"/>
					<int key="maxZoomLevel" value="14"/>
					<int key="topZoomLevel" value="18"/>
					<int key="tileSize" value="256"/>
				</properties>
			</layer>
			<layer id="World_Shaded_Relief" name="ArcGIS Shaded Relief">
				<className>nl.wldelft.libx.openmap.GenericTileServerLayer</className>
				<visible>true</visible>
				<properties>
					<string key="tileUrlPattern" value="http://server.arcgisonline.com/ArcGIS/rest/services/World_Shaded_Relief/MapServer/tile/%ZOOM%/%ROW%/%COLUMN%"/>
					<string key="cacheDir" value="$MAPLAYERSCACHE_FOLDER$/World_Shaded_Relief"/>
					<int key="minZoomLevel" value="1"/>
					<int key="maxZoomLevel" value="14"/>
					<int key="topZoomLevel" value="21"/>
					<int key="tileSize" value="256"/>
				</properties>
			</layer>
			<esriShapeLayer id="Bow Basins">
				<description>Bow Basins</description>
				<file>Bow_Basins.shp</file>
				<geoDatum>WGS 1984</geoDatum>
				<visible>true</visible>
				<toolTip>%NAME%</toolTip>
				<lineColor>black</lineColor>
				<fillRgbColor>FFFFFF</fillRgbColor>
				<opaquenessPercentage>20</opaquenessPercentage>
			</esriShapeLayer>
			<esriShapeLayer id="Bow HRUs / Sub Basins">
				<description>Bow Sub Basins</description>
				<file>Bow_SubBasins.shp</file>
				<geoDatum>WGS 1984</geoDatum>
				<visible>true</visible>
				<toolTip>%HRU_ID%</toolTip>
				<lineColor>black</lineColor>
				<fillRgbColor>FFFFFF</fillRgbColor>
				<opaquenessPercentage>20</opaquenessPercentage>
			</esriShapeLayer>
			<esriShapeLayer id="Bow Flow Lines">
				<description>Bow Flow lines</description>
				<file>Bow_Flowlines.shp</file>
				<geoDatum>WGS 1984</geoDatum>
				<visible>true</visible>
				<lineColor>blue</lineColor>
				<opaquenessPercentage>50</opaquenessPercentage>
			</esriShapeLayer>
			<esriShapeLayer id="World Countries">
				<description>World</description>
				<file>WorldCountries.shp</file>
				<geoDatum>WGS 1984</geoDatum>
				<visible>false</visible>
				<toolTip>%CNTRY_NAME%</toolTip>
				<lineColor>black</lineColor>
				<fillRgbColor>FFFFFF</fillRgbColor>
				<opaquenessPercentage>20</opaquenessPercentage>
			</esriShapeLayer>
		</geoMap>
		<timeDisplayLabels>bottomRight</timeDisplayLabels>
	</defaults>
	<defaults>
		<commentPositioning>topLeft</commentPositioning>
	</defaults>
	<gridPlotGroup id="WSC_Scalar_Download" name="Hydro Stations">
		<gridPlot id="RecentObservedDischarge" name="Recent Discharge[m3/s]">
			<timeSeriesSet>
				<moduleInstanceId>ImportWSC</moduleInstanceId>
				<valueType>scalar</valueType>
				<parameterId>QR.obs</parameterId>
				<locationSetId>DisplayHydroStations</locationSetId>
				<timeSeriesType>external historical</timeSeriesType>
				<timeStep unit="minute" multiplier="5"/>
				<relativeViewPeriod unit="day" start="-30" end="0"/>
				<readWriteMode>add originals</readWriteMode>
			</timeSeriesSet>
			<showThumbnailsPanel>false</showThumbnailsPanel>
			<classBreaksId>Class.Discharge</classBreaksId>
		</gridPlot>
		<gridPlot id="HistoricObservedDischarge" name="Historic Daily Discharge  [m3/s]">
			<timeSeriesSet>
				<moduleInstanceId>ImportWSC</moduleInstanceId>
				<valueType>scalar</valueType>
				<parameterId>QR.obs</parameterId>
				<locationSetId>DisplayHydroStations</locationSetId>
				<timeSeriesType>external historical</timeSeriesType>
				<timeStep id="$DAY_TIMESTEP$"/>
				<readWriteMode>read complete forecast</readWriteMode>
			</timeSeriesSet>
			<showThumbnailsPanel>false</showThumbnailsPanel>
			<classBreaksId>Class.Discharge</classBreaksId>
		</gridPlot>
	</gridPlotGroup>
	<gridPlotGroup id="ECCC_Scalar_Download" name="Meteo Stations">
		<gridPlot id="PrecipitationECCC" name="Precipitation Daily @ Stations [mm]">
			<dataLayer>
				<onlyCircles>false</onlyCircles>
				<circleSizeFunction>12</circleSizeFunction>
				<circleBorderSize>1</circleBorderSize>
				<circleBorderColor>gray</circleBorderColor>
				<timeSeriesSet>
					<moduleInstanceId>ImportECCCScalar</moduleInstanceId>
					<valueType>scalar</valueType>
					<parameterId>PC.obs</parameterId>
					<locationSetId>DisplayMeteoStations</locationSetId>
					<timeSeriesType>external historical</timeSeriesType>
					<timeStep id="$DAY_TIMESTEP$"/>
					<readWriteMode>read complete forecast</readWriteMode>
				</timeSeriesSet>
			</dataLayer>
			<showThumbnailsPanel>false</showThumbnailsPanel>
			<movieFrameDurationMillis>200</movieFrameDurationMillis>
			<movingAccumulationTimeSpan unit="day" multiplier="1"/>
			<movingAccumulationTimeSpan unit="day" multiplier="2"/>
			<movingAccumulationTimeSpan unit="day" multiplier="3"/>
			<movingAccumulationTimeSpan unit="day" multiplier="4"/>
			<movingAccumulationTimeSpan unit="day" multiplier="5"/>
			<movingAccumulationTimeSpan unit="day" multiplier="8"/>
			<classBreaksId>Class.Precipitation</classBreaksId>
		</gridPlot>
		<gridPlot id="TemperatureMaxECCC" name="Temperature Max Daily @ Stations [oC]">
			<dataLayer>
				<onlyCircles>false</onlyCircles>
				<circleSizeFunction>12</circleSizeFunction>
				<circleBorderSize>1</circleBorderSize>
				<circleBorderColor>gray</circleBorderColor>
				<timeSeriesSet>
					<moduleInstanceId>ImportECCCScalar</moduleInstanceId>
					<valueType>scalar</valueType>
					<parameterId>TAmax.obs</parameterId>
					<locationSetId>DisplayMeteoStations</locationSetId>
					<timeSeriesType>external historical</timeSeriesType>
					<timeStep id="$DAY_TIMESTEP$"/>
					<readWriteMode>read complete forecast</readWriteMode>
				</timeSeriesSet>
			</dataLayer>
			<classBreaksId>Class.Temperature</classBreaksId>
		</gridPlot>
		<gridPlot id="TemperatureMinECCC" name="Temperature Min Daily @ Stations [oC]">
			<dataLayer>
				<onlyCircles>false</onlyCircles>
				<circleSizeFunction>12</circleSizeFunction>
				<circleBorderSize>1</circleBorderSize>
				<circleBorderColor>gray</circleBorderColor>
				<timeSeriesSet>
					<moduleInstanceId>ImportECCCScalar</moduleInstanceId>
					<valueType>scalar</valueType>
					<parameterId>TAmin.obs</parameterId>
					<locationSetId>DisplayMeteoStations</locationSetId>
					<timeSeriesType>external historical</timeSeriesType>
					<timeStep id="$DAY_TIMESTEP$"/>
					<readWriteMode>read complete forecast</readWriteMode>
				</timeSeriesSet>
			</dataLayer>
			<classBreaksId>Class.Temperature</classBreaksId>
		</gridPlot>
	</gridPlotGroup>
	<gridPlotGroup id="Radar" name="Radar">
