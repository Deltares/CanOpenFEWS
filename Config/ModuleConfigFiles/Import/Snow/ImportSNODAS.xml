			<importTimeZone>
				<timeZoneName>GMT+0</timeZoneName>
			</importTimeZone>
			<!--<gridStartPoint>SW</gridStartPoint>-->
			<dataFeedId>SNODAS</dataFeedId>
		</general>
		<tolerance parameterId="SWE.sim" timeUnit="day" unitCount="1"></tolerance>
		<timeSeriesSet>
			<moduleInstanceId>ImportSNODAS</moduleInstanceId>
			<valueType>grid</valueType>
			<parameterId>SWE.sim</parameterId>
			<locationId>SNODAS</locationId>
			<timeSeriesType>external historical</timeSeriesType>
			<timeStep unit="day"/>
			<readWriteMode>add originals</readWriteMode>
			<synchLevel>6</synchLevel>
			<expiryTime unit="day" multiplier="15"/>
		</timeSeriesSet>
		<externUnit parameterId="SWE.sim" unit="kg.m-2"/>
