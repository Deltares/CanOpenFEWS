	<import>
		<!-- SWE Import -->
		<general>
			<importType>grib2</importType>
			<folder>$IMPORT_FOLDER$/SNODAS</folder>
			<fileNamePatternFilter>zz_ssmv11034tS__T0001TTNATS*05HP001.grib2</fileNamePatternFilter>
