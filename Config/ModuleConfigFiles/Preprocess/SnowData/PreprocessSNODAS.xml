<transformationModule xmlns="http://www.wldelft.nl/fews" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.wldelft.nl/fews http://fews.wldelft.nl/schemas/version1.0/transformationModule.xsd" version="1.0">
	<!--Input-->
	<variable>
		<variableId>GridNonEqui</variableId>
		<timeSeriesSet>
			<moduleInstanceId>Import$SOURCE$</moduleInstanceId>
			<valueType>grid</valueType>
			<parameterId>$PARAMETER$</parameterId>
			<locationId>$SOURCE$</locationId>
			<timeSeriesType>external historical</timeSeriesType>
			<timeStep unit="nonequidistant"/>
			<readWriteMode>read complete forecast</readWriteMode>
		</timeSeriesSet>
	</variable>
	<!--Temp-->
	<variable>
		<variableId>RegridNonEquiTemp</variableId>
		<timeSeriesSet>
			<moduleInstanceId>Import$SOURCE$</moduleInstanceId>
			<valueType>grid</valueType>
			<parameterId>$PARAMETER$</parameterId>
			<locationId>$DESTGRID$</locationId>
			<timeSeriesType>external historical</timeSeriesType>
			<timeStep unit="nonequidistant"/>
			<readWriteMode>read complete forecast</readWriteMode>
		</timeSeriesSet>
	</variable>
	<!--Output-->
	<variable>
		<variableId>Grid</variableId>
		<timeSeriesSet>
			<moduleInstanceId>Import$SOURCE$</moduleInstanceId>
			<valueType>grid</valueType>
			<parameterId>$PARAMETER$</parameterId>
			<locationId>$DESTGRID$</locationId>
			<timeSeriesType>external historical</timeSeriesType>
			<timeStep unit="day"/>
			<readWriteMode>read complete forecast</readWriteMode>
		</timeSeriesSet>
	</variable>
	<!--  SWE average / basin-->
	<variable>
		<variableId>Basin</variableId>
		<timeSeriesSet>
			<moduleInstanceId>Preprocess$SOURCE$</moduleInstanceId>
			<valueType>scalar</valueType>
			<parameterId>$PARAMETER$</parameterId>
			<locationSetId>ModelBasins</locationSetId>
			<timeSeriesType>external historical</timeSeriesType>
			<timeStep unit="day"/>
			<readWriteMode>read complete forecast</readWriteMode>
		</timeSeriesSet>
	</variable>
	<!--  SWE average / station-->
	<variable>
		<variableId>Station</variableId>
		<timeSeriesSet>
			<moduleInstanceId>Preprocess$SOURCE$</moduleInstanceId>
			<valueType>scalar</valueType>
			<parameterId>$PARAMETER$</parameterId>
			<locationSetId>SnowStations</locationSetId>
			<timeSeriesType>external historical</timeSeriesType>
			<timeStep unit="day"/>
			<readWriteMode>read complete forecast</readWriteMode>
		</timeSeriesSet>
	</variable>
	<!-- Transformation-->
	<transformation id="regrid">
	<interpolationSpatial>
	<closestDistance>
		<inputVariable>
				<variableId>GridNonEqui</variableId>
			</inputVariable>
			<distanceGeoDatum>$DISTANCEGEODATUM$</distanceGeoDatum>
			<outputVariable>
				<variableId>RegridNonEquiTemp</variableId>
		</outputVariable>
	</closestDistance>
	</interpolationSpatial>
	</transformation>
	<transformation id="sample_equidistant">
		<sample>
			<nonEquidistant>
				<nonEquidistantInputVariable>
					<variableId>RegridNonEquiTemp</variableId>
				</nonEquidistantInputVariable>
				<interpolationType>linear</interpolationType>
				<outputVariable>
					<variableId>Grid</variableId>
				</outputVariable>
			</nonEquidistant>
		</sample>
	</transformation>
	<transformation id="sample_basins">
		<interpolationSpatial>
			<average>
				<inputVariable>
					<variableId>Grid</variableId>
				</inputVariable>
				<outputVariable>
					<variableId>Basin</variableId>
				</outputVariable>
			</average>
		</interpolationSpatial>
	</transformation>
	<transformation id="sample_stations">
		<interpolationSpatial>
			<average>
				<inputVariable>
					<variableId>Grid</variableId>
				</inputVariable>
				<outputVariable>
					<variableId>Station</variableId>
				</outputVariable>
			</average>
		</interpolationSpatial>
	</transformation>
</transformationModule>